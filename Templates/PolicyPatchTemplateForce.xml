<?xml version="1.0" encoding="UTF-8"?>
<patch_policy>
	<general>
		<name>%patch_name%</name>
		<enabled>%enabled%</enabled>
		<target_version>%version%</target_version>
		<minimum_os>%min_os%</minimum_os>
		<kill_apps>
			<kill_app>
				<kill_app_name>%kill_app_name%</kill_app_name>
				<kill_app_bundle_id>%kill_app_bundle_id%</kill_app_bundle_id>
			</kill_app>
		</kill_apps>
		<distribution_method>prompt</distribution_method>
		<allow_downgrade>false</allow_downgrade>
		<patch_unknown>true</patch_unknown>
	</general>
	<scope>
		<all_computers>true</all_computers>
		<computers />
		<computer_groups />
		<users />
		<buildings />
		<departments />
		<limitations>
			<network_segments />
			<ibeacons />
		</limitations>
		<exclusions>
			<computers />
			<computer_groups />
			<users />
			<buildings />
			<departments />
			<network_segments />
			<ibeacons />
		</exclusions>
	</scope>
	<user_interaction>
		<grace_period>
			<grace_period_duration>60</grace_period_duration>
			<notification_center_subject>%NAME%</notification_center_subject>
			<message>%NAME% will quit in $DELAY_MINUTES minutes for an update.</message>
		</grace_period>
	</user_interaction>
	<software_title_configuration_id>%patch_softwaretitle_id%</software_title_configuration_id>
</patch_policy>